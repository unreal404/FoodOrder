<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.edu.neusoft.zw725.foodorder">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- 15110100725 周玮 -->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.RegActivity" />
        <activity android:name=".activity.FoodActivity" />
        <activity android:name=".activity.FoodDetailedActivity" />
        <activity android:name=".activity.FoodByActivity" />
        <activity android:name=".activity.BaseActivity"></activity>
        <activity android:name=".activity.ReviseActivity"></activity>
    </application>

</manifest>